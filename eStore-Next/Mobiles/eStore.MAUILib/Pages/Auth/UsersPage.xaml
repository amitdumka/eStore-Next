<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="eStore.MAUILib.Pages.Auth.UsersPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:rlv="clr-namespace:eStore.MAUILib.Views.Custom"
    xmlns:viewModels="clr-namespace:eStore.MAUILib.ViewModels.Auth"
    Title="{Binding Title, Mode=TwoWay}">

    <VerticalStackLayout>
        <rlv:RecordListingView
            x:Name="RLV"
            AddButtonText="Add User"
            BackgroundColor="White"
            RefreshButtonText="ReLoad" />
    </VerticalStackLayout>
</ContentPage>
