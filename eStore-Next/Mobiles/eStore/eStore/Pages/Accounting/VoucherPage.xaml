<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="eStore.Pages.Accounting.VoucherPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:dxe="clr-namespace:DevExpress.Maui.Editors;assembly=DevExpress.Maui.Editors"
    xmlns:rlv="clr-namespace:eStore.MAUILib.Views.Custom;assembly=eStore.MAUILib"
    Title="{Binding Title, Mode=TwoWay}">
    <VerticalStackLayout>
        <HorizontalStackLayout>
            <dxe:ComboBoxEdit ItemsSource="{Binding VoucherType}" />
        </HorizontalStackLayout>
        <rlv:RecordListingView x:Name="RLV" AddButtonText="Add" />
    </VerticalStackLayout>
</ContentPage>
